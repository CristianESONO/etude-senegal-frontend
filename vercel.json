{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/admin/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/etudier",
      "destination": "/index.html"
    },
    {
      "source": "/formalites",
      "destination": "/index.html"
    },
    {
      "source": "/loger",
      "destination": "/index.html"
    },
    {
      "source": "/etablissements/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { 
          "key": "Cache-Control", 
          "value": "public, max-age=31536000, immutable" 
        }
      ]
    }
  ]
}